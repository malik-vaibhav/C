  int main()
 {
 int a;
 int b;
 int c;
 printf("Enter the value of a-");
  scanf("%d", &a);
  
  printf("Enter the value of b-");
  scanf("%d", &b);
  
 printf("enter the value of c-");
  scanf("%d", &c);
  
 if(a>b && a>c)
 {
  printf("A is the greatest");
  }
 if(b>a && b>c)
 {
  printf("B is the greatest");
  }
  else
  {
  printf("C is the greatest");
  }
  getch();
